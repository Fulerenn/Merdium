{
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "root": true,
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "@typescript-eslint/no-var-requires": 0,
        "@typescript-eslint/no-empty-function": 0
    }
}
